{
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "delparque.agendable.io"
                }
            ],
            "destination": "/a/delparque/:path*"
        },
        {
            "source": "/api/:path*",
            "has": [
                  {
                    "type": "host",
                    "value": "(?<subdomain>.*)"
                  }
            ],
            "destination": "http://:agendable.io:/a/:subdomain/:path*"
          },
        {
            "source": "/a/",
            "destination": "https://app.agendable.io/a/"
        },
        {
            "source": "/a/:match*",
            "destination": "https://app.agendable.io/a/:match*"
        },
        {
            "source": "/app",
            "destination": "https://app.agendable.io/"
        },
        {
            "source": "/app/:match*",
            "destination": "https://app.agendable.io/:match*"
        },
        {
            "source": "/",
            "destination": "https://landing.agendable.io/"
        },
        {
            "source": "/:match*",
            "destination": "https://landing.agendable.io/:match*"
        }
    ]
}